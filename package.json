{
  "name": "keystone-boilerplate",
  "version": "0.0.5",
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "axios-retry": "^2.0.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "^0.0.3",
    "blister": "^1.1.0",
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "easy-login": "^1.0.0",
    "express-marko": "^1.4.0",
    "keystone": "^4.0.0-beta.5",
    "lodash": "^4.17.4",
    "require-dir": "^0.3.2",
    "restful-keystone": "^0.3.0"
  },
  "devDependencies": {
    "bulma": "^0.5.3",
    "husky": "^0.14.3",
    "jquery": "^3.2.1",
    "lint-staged": "^4.0.2",
    "node-dev": "^3.1.3",
    "onchange": "^3.2.1",
    "prettier": "^1.5.3",
    "envify": "^4.1.0",
    "markoify": "^2.2.2",
    "rollup": "^0.45.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sass": "^0.5.3",
    "rollup-plugin-browserify-transform": "^0.1.0",
    "rollup-plugin-commonjs": "^8.2.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ]
  },
  "babel-server": {
    "presets": [
      [
        "env"
      ]
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "node index.js",
    "develop": "npm run develop:server",
    "develop:server": "node-dev --respawn --no-notify --no-deps --inspect=0.0.0.0:5858 index.js",
    "build": "rollup -c",
    "develop:client": "onchange 'client/**/*' -i -- npm run build"
  },
  "lint-staged": {
    "server/**/*.js": [
      "node_modules/.bin/prettier --single-quote 'server/**/*.js' --write",
      "git add"
    ]
  },
  "main": "index.js"
}
